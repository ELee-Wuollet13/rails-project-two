<div class="row">
<div class="col-1">
</div>
  <div class="col-6">



<h1>Three Most Recently Added Products</h1>

<% if @products.any? %>
<ul>
  <% @products.each do |product| %>
  <li><%= link_to product.name, product_path(product) %></li>
  <% end %>
</ul>
<% else %>
<p>Product List is Empty</p>
<% end %>

</div>
<div class="col-5">
  <p><%= link_to "Return to index", products_path %></p>

</div>
</div>
